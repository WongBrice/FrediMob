<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fredi_background">

    <!--TRAJET-->
    <TextView
        android:id="@+id/Trajet"
        android:textSize="16sp"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="40dp"
        android:background="@color/colorGreen"
        android:textColor="@color/colorBlank"
        android:text="@string/fraisTrajet"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/trajet"
        android:textSize="16sp"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="40dp"
        android:layout_marginBottom="25sp"
        android:textColor="@color/colorBlank"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>


    <!-- KM -->
    <TextView
        android:id="@+id/Km"
        android:textColor="@color/colorBlank"
        android:textSize="17sp"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="40dp"
        android:background="@color/colorGreen"
        android:text="@string/fraisKm"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/km"
        android:textSize="16sp"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="40dp"
        android:textColor="@color/colorBlank"
        android:layout_width="match_parent"
        android:layout_marginBottom="25sp"
        android:layout_height="wrap_content"/>

    <!-- PEAGE -->
    <TextView
        android:id="@+id/Peage"
        android:textColor="@color/colorBlank"
        android:textSize="17sp"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="40dp"
        android:background="@color/colorGreen"
        android:text="@string/fraisPeage"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/peage"
        android:textSize="16sp"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="40dp"
        android:layout_marginBottom="25sp"
        android:textColor="@color/colorBlank"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <!-- REPAS -->
    <TextView
        android:id="@+id/Repas"
        android:textColor="@color/colorBlank"
        android:textSize="17sp"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="40dp"
        android:background="@color/colorGreen"
        android:text="@string/fraisRepas"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
